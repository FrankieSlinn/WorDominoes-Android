{
  "expo": {
    "name": "WorDominoes",
    "slug": "WorDominoes",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/FaviconWD.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "packagerOpts": {
      "hostType": "lan"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fkissling.WorDominoes",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/FaviconWD"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/FaviconWD.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1597a3f-3939-4573-97c8-78bea62a860c"
      }
    }
  }
}
